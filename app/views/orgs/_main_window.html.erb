


  <div class="org-main-window-panel">
      


    <div class ="org-info-display"  id="org-homepage">
     
      <div class="org-info-list">
        <ol> 
          <li><span><%=t("Mission")%></span>
            <p>
              <%= @org.mission %>
            </p></li>

          <li><span><%=t("Causes") %></span>  
            <p>
            <%= @org.causes.map { |cause| cause.description }.add_commas %>
            </p></li>
            
          <li><span><%=t("Objectives")%></span>
            <p>
            <% @org.objectives.each do |obj| %>
             <p><%= obj.summary %></p>
             <% end %>
            </p></li>

          <li><span><%=t("Activities")%></span>
            <p>
              <%= @org.activities.map { |activity| activity.description }.add_commas %>
            </p></li>

          <li><span><%=t("Staff")%></span>
            <p>
              <%= render partial: 'staff_table' %>
            </p></li>

          <li><span><%=t("Branches")%></span>
             <p>
              <% @org.branches.each do |branch| %>
              <p><%= branch.description %></p>
              <% end %>
            </p></li>

          <li><span><%=t(:Age_Group)%></span>
             <p>
            <%= @org.ages.map { |age| age.description }.add_commas %>
            </p></li>

          <li><span><%=t("Other")%></span>
             <p>
                <b><%="Founding Date"+": "%><font class="org-text-padding"> </b><%=@org.founding_date.strftime("%d-%m-%Y") %></font><br>
                <b><%="Starting Date"+": "%><font class="org-text-padding"> </b><%= @org.starting_date.strftime("%d-%m-%Y") %></font><br>
                <b><%="Type of Legal Entity"+": "%><font class="org-text-padding"> </b><%= @org.legal_type %></font><br>
                <b><%="Legal Entity No"+": "%><font class="org-text-padding"> </b><%= @org.legal_num %></font><br> 
                <b><%="CUIT:   " %><font class="org-text-padding"> </b><%= @org.cuit %></font></br>
            </p></li>
         


          <li><span><%="Nothing here"%></span>
             <p> 
              <% if @org.trans_policy %>
              <%= image_tag("check.png", class: "check-or-cross") %>
              <%else%>
              <%= image_tag("cross.png", class: "check-or-cross") %>
              <% end %>
              <span class="org-text-padding">Written Transparency Policy</span>
            </p>
   

<!-- 
              t.string :trans_policy
      t.boolean :code_conduct
      t.string :external_auditor
      t.string :auditor_name
      t.string :annual_reporting
      t.string :external_prog_eval
              [@org.trans_policy,@org.code_conduct, @org.external_auditor] -->
        
            </p></li>


          




        </ol> 
     </div>
 
   </div>


